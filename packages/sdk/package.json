{
  "name": "@ermisnetwork/ermis-classroom-sdk",
  "version": "0.1.4",
  "description": "TypeScript SDK for Ermis Classroom - Real-time video conferencing and collaboration",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite-plugin": {
      "types": "./vite-plugin.d.ts",
      "import": "./vite-plugin.js"
    },
    "./workers/*": "./src/workers/*",
    "./raptorQ/*": "./src/raptorQ/*",
    "./polyfills/*": "./src/polyfills/*",
    "./opus_decoder/*": "./src/opus_decoder/*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src/workers",
    "src/raptorQ",
    "src/polyfills",
    "src/opus_decoder",
    "vite-plugin.js",
    "vite-plugin.d.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist .turbo node_modules"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "tsup": "^8.0.0",
    "typescript": "^5.8.2",
    "vite": "^7.1.7",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ermisnetwork/ermis-classroom-sdk",
    "directory": "packages/sdk"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2"
  }
}
